                        <!-- Edit Personal Info -->
                        <div class="widget personal-info">
                            <h3 class="widget-header user">Agent Registration</h3>
                            <form action="/Agent/Register" method="post" enctype="multipart/form-data">
                                <!-- First Name -->
                                <div class="form-group">
                                    <label for="first-name">Name</label>
                                    <input type="text" class="form-control" id="first-name" name="name" required>
                                </div>
                                <!-- Last Name -->
                                <div class="form-group">
                                    <label for="last-name">Email</label>
                                    <input type="email" class="form-control" id="last-name" name="email" required>
                                </div>
                                <!-- File chooser -->
                                <!-- Comunity Name -->
                                <div class="form-group">
                                    <label for="comunity-name">Password</label>
                                    <input type="password" class="form-control" id="password" name="password" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters" onkeyup='check();' required>
                                </div>
                                <div id="message">
                                    <h3>Password must contain the following:</h3>
                                    <p id="letter" class="invalid">A <b>lowercase</b> letter</p>
                                    <p id="capital" class="invalid">A <b>capital (uppercase)</b> letter</p>
                                    <p id="number" class="invalid">A <b>number</b></p>
                                    <p id="length" class="invalid">Minimum <b>8 characters</b></p>
                                </div>
                                <div class="form-group">
                                    <label for="comunity-name">Confirm Password</label>
                                    <input type="password" class="form-control" id="confirmpassword" name="cpassword" onkeyup='check();' required>
                                </div>
                                <p id="matched"></p>
                                <div class="form-group">
                                    <label for="comunity-name">Phone No.</label>
                                    <input type="tel" class="form-control" id="tel" name="tel" required>
                                </div>

                                <div class="form-group">
                                    <label for="comunity-name">Address</label>
                                    <input type="text" class="form-control" id="comunity-name" name="address" required>
                                </div>
                                
                        
                                <select class="form-control" name="area1" id="areas" required>
                                    <option selected disabled>Select Area</option>
                                    @for (int i = 0; i < AgentArea.TableAea().Rows.Count; i++)
                                    {

                                        <option value="@AgentArea.TableAea().Rows[i][0].ToString()">@AgentArea.TableAea().Rows[i][1].ToString()</option>
                                    }

                                </select>
                                <br />

                        <br />
                                <select multiple name="selectblock" id="blocks" hidden style="visibility:hidden" required></select>

                        <br>
                        <br>
                        <select class="form-control" name="area2" id="areas2">
                            <option disabled selected>Select Area</option>
                            @for (int i = 0; i < AgentArea.TableAea().Rows.Count; i++)
                            {

                                <option value="@AgentArea.TableAea().Rows[i][0].ToString()">@AgentArea.TableAea().Rows[i][1].ToString()</option>
                            }

                        </select>
                        <br />
                                <br />
                        <select multiple name="selectblock2" style="visibility:hidden" id="blocks2"></select>
                        <br>
                        <br>
                        <select name="area3" class="form-control" id="areas3">
