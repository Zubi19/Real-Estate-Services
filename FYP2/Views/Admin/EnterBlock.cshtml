    <section class="user-profile section">
        <div class="container">
            <div class="row">

                <div class="col-md-10 offset-md-1 col-lg-8 offset-lg-0">
                    <!-- Edit Personal Info -->
                    <div class="widget personal-info">
                        <h3 class="widget-header user">Enter Blocks</h3>
                        <form action="/Admin/AdminEnterBlock" method="post">
                            <!-- First Name -->
                            <div class="form-group">
                                <label for="first-name">Select Area</label>
                                <select class="form-control" name="area" id="areas">
                                    <option disabled selected>Select Area</option>
                                    @for (int i = 0; i < AgentArea.TableAea().Rows.Count; i++)
                                    {

                                        <option value="@AgentArea.TableAea().Rows[i][0].ToString()">@AgentArea.TableAea().Rows[i][1].ToString()</option>
                                    }

                                </select>
                                @*<input type="text" class="form-control" id="first-name" name="name">*@
                            </div>
                            <!-- Last Name -->
                            <div class="form-group">
                                <label for="last-name">Enter Blocks</label>

                                <input type="text" class="form-control" id="last-name" name="block">
                            </div>
                            <!-- File chooser -->
                            <!-- Comunity Name -->
                            <!-- Submit button -->
                            <button type="submit" class="btn btn-transparent">Submit</button>
                        </form>
                    </div>


                </div>
